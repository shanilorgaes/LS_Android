<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/main_lay"
    android:layout_height="match_parent"
    android:layout_centerInParent="true"
    android:layout_gravity="bottom"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/ads_finished_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/asdsfinished">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline170"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".1" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline171"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".17" />

            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".83" />

            <ImageView
                android:id="@+id/ic_close"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/ic_closebutton"
                app:layout_constraintEnd_toStartOf="@+id/guideline169"
                app:layout_constraintTop_toTopOf="@+id/guideline170" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline169"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".9" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline172"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="48dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline174"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".35" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline175"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".65" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline181"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.705" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline180"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".62" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline186"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.7" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline182"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.555" />

            <ImageView
                android:id="@+id/coinbox"
                android:layout_width="38dp"
                android:layout_height="55dp"
                android:background="@drawable/coinbox_icons"
                app:layout_constraintBottom_toTopOf="@+id/guideline182"
                app:layout_constraintEnd_toStartOf="@+id/guideline181"
                app:layout_constraintStart_toStartOf="@+id/guideline180"
                app:layout_constraintTop_toTopOf="@+id/guideline179" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline179"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.43" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/goodluck"
                app:layout_constraintBottom_toTopOf="@+id/guideline173"
                app:layout_constraintEnd_toStartOf="@+id/guideline175"
                app:layout_constraintStart_toStartOf="@+id/guideline174"
                app:layout_constraintTop_toTopOf="@+id/guideline172" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline173"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".35" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline178"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".38" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline176"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".155" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/coincover"
                app:layout_constraintBottom_toTopOf="@+id/guideline177"
                app:layout_constraintEnd_toStartOf="@+id/guideline174"
                app:layout_constraintStart_toStartOf="@+id/guideline176"
                app:layout_constraintTop_toTopOf="@+id/guideline178" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline177"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".55" />

            <TextView
                android:id="@+id/coin"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text=""
                android:gravity="center"
                android:textColor="@color/DarkGray"
                android:textSize="10dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline175"
                app:layout_constraintStart_toStartOf="@+id/guideline174"
                app:layout_constraintTop_toTopOf="@+id/guideline178" />

            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="@drawable/lines"
                app:layout_constraintEnd_toStartOf="@+id/guideline175"
                app:layout_constraintStart_toStartOf="@+id/guideline174"
                app:layout_constraintTop_toBottomOf="@+id/coin" />

            <TextView
                android:id="@+id/coin2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:maxLines="1"
                android:text=""
                android:textColor="#673AB7"
                android:textSize="10dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline180"
                app:layout_constraintStart_toStartOf="@+id/guideline174"
                app:layout_constraintTop_toBottomOf="@+id/coin" />

            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="@drawable/lines"
                app:layout_constraintEnd_toStartOf="@+id/guideline175"
                app:layout_constraintStart_toStartOf="@+id/guideline174"
                app:layout_constraintTop_toBottomOf="@+id/coin2" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/more_ncoins_luck"
                android:textColor="@color/DarkGray"
                android:textSize="12dp"
                app:layout_constraintBottom_toTopOf="@+id/guideline545"
                app:layout_constraintEnd_toStartOf="@+id/guideline175"
                app:layout_constraintStart_toStartOf="@+id/guideline174"
                app:layout_constraintTop_toTopOf="@+id/guideline177" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline183"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".88" />

            <ImageView
                android:layout_width="42dp"
                android:layout_height="42dp"
                android:background="@drawable/share_icon_home"
                app:layout_constraintBottom_toTopOf="@+id/guideline184"
                app:layout_constraintStart_toStartOf="@+id/guideline186" />


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline184"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.915" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline187"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".93" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline185"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.799" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline545"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".63" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/scanluck"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginEnd="10dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/scan_luck_bg"
                app:layout_constraintBottom_toTopOf="@+id/guideline187"
                app:layout_constraintEnd_toStartOf="@+id/guideline175"
                app:layout_constraintStart_toStartOf="@+id/guideline174"
                app:layout_constraintTop_toTopOf="@+id/guideline545">

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline190"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent=".225" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline192"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent=".8" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline189"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".845" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline188"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".165" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/scanluck"
                    app:layout_constraintBottom_toTopOf="@+id/guideline192"
                    app:layout_constraintEnd_toStartOf="@+id/guideline189"
                    app:layout_constraintStart_toStartOf="@+id/guideline188"
                    app:layout_constraintTop_toTopOf="@+id/guideline190" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/bottom_colorimage"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/guideline183" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/asdsfinished"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent=".48" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
